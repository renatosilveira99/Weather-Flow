datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model WeatherCache {
  id                      String @id @default(uuid())
  city                    String
  date                    String
  temperatureInCelsius    Float
  temperatureInFahrenheit Float

  @@unique([city, date])
}
